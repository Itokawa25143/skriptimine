#!/bin/bash
$ bash -x script.sh 2> log
# Prooov
clear
#
echo -n "Sisesta veebileht: "
read veebileht
nmap="nmap --open $veebileht"
$nmap
# Kysime, kas kasutaja soovib tulemusi txt failina salvestada?
echo "Kas soovite faili .txt failina salvestada?"
read -p "Valige (Y/N): "
if [ "$Valige" = "Y" ]; then
 touch ./scan2 > $veebileht.txt
 echo "Fail on salvestatud."
else
 echo "Faili ei salvestatud"
 exit
 echo
fi

